{
  "name": "kuzzle-plugin-mqtt",
  "author": "The Kuzzle Team <support@kuzzle.io>",
  "version": "2.0.1",
  "description": "Kuzzle protocol plugin MQTT",
  "main": "./lib/index.js",
  "scripts": {
    "test": "npm run --silent lint && npm run unit-testing --coverage",
    "unit-testing": "istanbul test _mocha",
    "lint": "eslint --max-warnings=0 ./lib ./test"
  },
  "pluginInfo": {
    "defaultConfig": {
      "port": 1883,
      "requestTopic": "Kuzzle/request",
      "responseTopic": "Kuzzle/response",
      "allowPubSub": false
    }
  },
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "mosca": "^2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kuzzleio/kuzzle-plugin-mqtt.git"
  },
  "devDependencies": {
    "codecov": "^1.0.1",
    "eslint": "3.4.0",
    "istanbul": "0.4.5",
    "istanbul-middleware": "0.2.2",
    "mocha": "3.0.2",
    "mock-require": "^1.3.0",
    "proxyquire": "^1.7.10",
    "rewire": "2.5.2",
    "should": "11.1.0",
    "should-sinon": "0.0.5",
    "sinon": "1.17.5",
    "sinon-as-promised": "^4.0.2"
  },
  "engines": {
    "node": ">= 4.4.5",
    "npm": ">= 2.14.4"
  },
  "license": "Apache-2.0"
}
